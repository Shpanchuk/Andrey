    "# 5 or 3\n",
    "def summa(N):\n",
    "    sum = 0\n",
    "    for i in range(N + 1):\n",
    "        if ((i % 5) == 0) or ((i % 3) == 0):\n",
    "            sum = sum + i\n",
    "    return (sum)"
   ]
